target C {
    platform: "Pico",
    threading: false,
    timeout: 10 sec,
    #no-compile: true,
    #cmake-include: ["../platform/pico_config.txt"] 
}

preamble {=
    #include <stdio.h>
    #include "pico/stdlib.h"
=}

main reactor {
    timer t1(0, 1 sec); 
    reaction(t1) {=
        printf("Hello World!\n");
    =}
}
